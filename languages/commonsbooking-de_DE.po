#: includes/Options.php:214
msgid ""
"\n"
"                            Hi {{user:first_name}},<br>\n"
"                            <p>thank you for booking {{item:post_title}} "
"{{booking:booking_timeframe_date}}.\n"
"                            </p>\n"
"                            \n"
"                            Pick up date and time: <b>{{booking:"
"pickupDatetime}}</b><br>\n"
"                            {{location:pickupInstructions}}\n"
"                            {{booking:formattedBookingCode}}\n"
